	void go(String[] args){
		//File infile = new File();
		
		if(args.length>0){
			File infile = new File(args[0]);
			try{
				Scanner readNum = new Scanner(infile);
			}
			catch(FileNotFoundException s){
			}
			System.out.println("got the file");			
		}
		else{
			System.exit(0);
		}
		
	}

